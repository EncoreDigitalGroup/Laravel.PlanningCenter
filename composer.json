{
    "name": "encoredigitalgroup/laravel-planningcenter",
    "type": "library",
    "description": "Laravel Wrapper for EncoreDigitalGroup/PlanningCenter-PHP-SDK",
    "keywords": [
        "encoredigitalgroup",
        "laravel",
        "laravel-planningcenter"
    ],
    "homepage": "https://github.com/encoredigitalgroup/laravel-planningcenter",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Marc Beinder",
            "email": "marc@marcbeinder.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "encoredigitalgroup/planningcenter-php-sdk": "v1.0.0-rc8",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "tightenco/duster": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "EncoreDigitalGroup\\Laravel\\PlanningCenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EncoreDigitalGroup\\Laravel\\PlanningCenter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "vendor/bin/duster fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EncoreDigitalGroup\\Laravel\\PlanningCenter\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
